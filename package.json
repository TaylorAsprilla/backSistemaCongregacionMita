{
  "name": "backsistemacongregacionmita",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "build": "tsc && npm run copy-templates && npm run copy-assets",
    "build:dev": "tsc -w",
    "start:dev": "nodemon build/app.js",
    "start": "node build/app.js",
    "deploy:prod": "eb deploy Sistemacmi-env",
    "dist": "sh ./scripts/dist.sh",
    "clean": "rm -rf build",
    "clean-modules": "rm -rf node_modules package-lock.json",
    "copy-templates": "copyfiles src/templates/* build/",
    "copy-assets": "copyfiles src/assets/**/* build/",
    "docker:build": "bash docker/scripts/docker-commands.sh build",
    "docker:up": "bash docker/scripts/docker-commands.sh up",
    "docker:down": "bash docker/scripts/docker-commands.sh down",
    "docker:logs": "bash docker/scripts/docker-commands.sh logs",
    "docker:clean": "bash docker/scripts/docker-commands.sh clean",
    "docker:db-only": "bash docker/scripts/docker-commands.sh db-only",
    "docker:reset": "bash docker/scripts/docker-commands.sh reset",
    "docker:help": "bash docker/scripts/docker-commands.sh help"
  },
  "keywords": [
    "Congregación Mita",
    "Mita",
    "Servicios"
  ],
  "author": "Congregación Mita INC",
  "license": "ISC",

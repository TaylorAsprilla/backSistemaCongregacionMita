    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^7.0.1",
    "generate-password": "^1.7.0",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.16.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^8.0.0",
    "nodemon": "^3.1.11",
    "qrcode": "^1.5.4",
    "rimraf": "^5.0.1",
    "sequelize": "^6.17.0",
    "sharp": "^0.34.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.6",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^4.6.2"
